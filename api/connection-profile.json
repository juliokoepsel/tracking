{
  "name": "delivery-network",
  "version": "1.0.0",
  "client": {
    "organization": "DeliveryOrg",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "channels": {
    "deliverychannel": {
      "orderers": [
        "orderer.example.com"
      ],
      "peers": {
        "peer0.delivery.example.com": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        }
      }
    }
  },
  "organizations": {
    "DeliveryOrg": {
      "mspid": "DeliveryOrgMSP",
      "peers": [
        "peer0.delivery.example.com"
      ],
      "certificateAuthorities": [
        "ca.delivery.example.com"
      ]
    }
  },
  "orderers": {
    "orderer.example.com": {
      "url": "grpcs://orderer.example.com:7050",
      "tlsCACerts": {
        "path": "/app/organizations/ordererOrganizations/example.com/orderers/orderer.example.com/msp/tlscacerts/tlsca.example.com-cert.pem"
      },
      "grpcOptions": {
        "ssl-target-name-override": "orderer.example.com"
      }
    }
  },
  "peers": {
    "peer0.delivery.example.com": {
      "url": "grpcs://peer0.delivery.example.com:7051",
      "tlsCACerts": {
        "path": "/app/organizations/peerOrganizations/delivery.example.com/peers/peer0.delivery.example.com/tls/ca.crt"
      },
      "grpcOptions": {
        "ssl-target-name-override": "peer0.delivery.example.com",
        "grpc.keepalive_time_ms": 600000
      }
    }
  },
  "certificateAuthorities": {
    "ca.delivery.example.com": {
      "url": "https://ca.delivery.example.com:7054",
      "tlsCACerts": {
        "path": "/app/organizations/peerOrganizations/delivery.example.com/ca/ca.delivery.example.com-cert.pem"
      },
      "caName": "ca-deliveryorg",
      "httpOptions": {
        "verify": true
      }
    }
  }
}
